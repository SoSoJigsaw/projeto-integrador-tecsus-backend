{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "reporters": [
    "default",
    [
      "jest-junit",
      {
        "outputDirectory": "./test-results/e2e",
        "outputName": "junit.xml"
      }
    ]
  ]
}
